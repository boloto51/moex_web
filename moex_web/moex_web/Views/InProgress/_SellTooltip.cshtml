<div class="sell-tooltip-wrapper hidden-element">
    <div class="sell-tooltip">
        <label class="title"></label>
        <div class="input-group date">
            <!--<label>Sell date</label>-->
            <input type="text" class="form-control date-input">
            <div class="input-group-addon">
                <span class="glyphicon glyphicon-th"></span>
            </div>
        </div>
        <div class="input-group price">
            <!--<label>Price</label>-->
            <input type="number" class="form-control price-input" placeholder="Price">
            <div class="validation price-validation hidden-element">Wrong price!</div>
        </div>
        <div class="input-group lotcount">
            <!--<label>LotCount</label>-->
            <input type="number" class="form-control lotcount-input" disabled>
        </div>
        <div class="manage-buttons">
            <button class="confirm">Sell</button>
            <button class="cancel">Cancel</button>
        </div>
    </div>
</div>

