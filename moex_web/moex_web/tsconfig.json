{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node"
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "app/scripts/**/*.ts"
  ]
}
